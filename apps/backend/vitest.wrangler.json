{
	"name": "sitelink-backend-test",
	"main": "src/test-worker.ts",
	"compatibility_date": "2024-12-01",
	"compatibility_flags": ["nodejs_compat", "nodejs_compat_v2"],
	"rules": [
		{
			"type": "CompiledWasm",
			"globs": ["**/*.wasm"],
			"fallthrough": true
		}
	],
	"define": {
		"process.env.NODE_ENV": "\"test\""
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "sitelink-db-test",
			"database_id": "test-db-id"
		}
	],
	"r2_buckets": [
		{
			"binding": "R2_BUCKET",
			"bucket_name": "sitelink-files-test"
		}
	],
	"queues": {
		"producers": [
			{
				"binding": "R2_NOTIFICATION_QUEUE",
				"queue": "sitelink-r2-notifications-test"
			},
			{
				"binding": "IMAGE_GENERATION_QUEUE",
				"queue": "sitelink-image-generation-test"
			},
			{
				"binding": "METADATA_EXTRACTION_QUEUE",
				"queue": "sitelink-metadata-extraction-test"
			},
			{
				"binding": "CALLOUT_DETECTION_QUEUE",
				"queue": "sitelink-callout-detection-test"
			},
			{
				"binding": "TILE_GENERATION_QUEUE",
				"queue": "sitelink-tile-generation-test"
			}
		]
	},
	"durable_objects": {
		"bindings": [
			{
				"name": "PLAN_COORDINATOR_DO",
				"class_name": "PlanCoordinator"
			}
		]
	},
	"migrations": [
		{
			"tag": "v1",
			"new_classes": ["PlanCoordinator"]
		}
	]
}
