{
	"name": "sitelink-backend-test",
	"main": "src/test-worker.ts",
	"compatibility_date": "2024-12-01",
	"compatibility_flags": ["nodejs_compat", "nodejs_compat_v2"],
	"rules": [
		{
			"type": "CompiledWasm",
			"globs": ["**/*.wasm"],
			"fallthrough": true
		}
	],
	"define": {
		"process.env.NODE_ENV": "\"test\""
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "sitelink-db-test",
			"database_id": "test-db-id"
		}
	],
	"r2_buckets": [
		{
			"binding": "R2_BUCKET",
			"bucket_name": "sitelink-files-test"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"name": "PDF_PROCESSOR",
				"class_name": "TestPdfProcessor"
			},
			{
				"name": "LIVESTORE_CLIENT_DO",
				"class_name": "LiveStoreCollector"
			}
		]
	},
	"workflows": [
		{
			"name": "plan-processing-test",
			"binding": "PLAN_PROCESSING_WORKFLOW",
			"class_name": "PlanProcessingWorkflow"
		}
	],
	"migrations": [
		{
			"tag": "v1",
			"new_classes": ["TestPdfProcessor", "LiveStoreCollector"]
		}
	]
}
