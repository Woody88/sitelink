{
  "name": "sitelink-interpreter",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "ingest": "bun run src/ingestion/index.ts",
    "extract": "bun run src/extraction/index.ts",
    "synthesize": "bun run src/synthesis/index.ts",
