  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "nanoid": "^5.1.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
